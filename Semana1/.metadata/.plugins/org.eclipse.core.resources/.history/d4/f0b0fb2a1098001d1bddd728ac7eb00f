package cam.curso.v0;

public class Becario {
	
	//has -a   tiene una computadora
	String nombre;
	CompuWindows computadora;

	public Becario(String nombre) {
		super();
		this.nombre = nombre;
	}

	@Override
	public String toString() {
		return "Becario [nombre=" + nombre + "]";
	}
	
	void encenderCompu(){
		System.out.println(nombre);
		computadora.prender();
	}
	
	

}
