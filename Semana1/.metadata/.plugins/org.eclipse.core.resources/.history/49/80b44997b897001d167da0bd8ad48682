package com.agencia;

public class Principal {

	public static void main(String[] args) {
		
	  Motor m1 = new MotorElectrico();
	  Motor m2 = new MotorGasolina();
	  
	  Auto auto1 = new Mazda(m1);
	  Auto auto2 = new Mazda(m2);
	  
	  auto1.encender();
	  auto1.describirAuto();
	  auto1.apagar();
	  
	  auto2.encender();
	  auto2.describirAuto();
	  auto2.apagar();

	}

}
