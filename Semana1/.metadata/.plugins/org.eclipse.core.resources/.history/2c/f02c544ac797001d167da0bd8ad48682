package com.singleton;

public class BitacoraInventario {
	
	private static BitacoraInventario bitacora;
	
	private BitacoraInventario() {}
	
	public static BitacoraInventario getBitacora() {
		if (bitacora == null) {
			bitacora = new BitacoraInventario();
		}
		return bitacora;
	}
	
	public void imprimirMovimiento(String movimiento) {
		System.out.println(movimiento);
	}
}
